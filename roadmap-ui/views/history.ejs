<% layout('./boilerplate') %>

<h1 class="mb-4">History</h1>

<ul class="list-group col-5">
    <% for (point of waypoints) { %>
    <li class="list-group-item d-flex justify-content-between align-items-start">
        <div class="ms-2 me-auto">
            <div class="fw-bold"><%= point.title %></div>
            <%= point.kind %>
        </div>
    </li>
    <% } %>
</ul>